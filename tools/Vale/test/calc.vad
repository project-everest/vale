include "common.vad"

#verbatim
function F(x: int): int { x + 25 }
function G(x: int, y: int): int { x + y }
#endverbatim

procedure{:refined} Q()
    requires
        true;
    ensures
        true;
{
    calc {
      F(100);
      100 + 25;
      { assert 100 + 25 == 120 + 5; }
      120 + 5;
    <= { assert true; }
      G(120, 5);
    == { assert true ==> true; }
      { assert false ==> 5 < 10; }
      G(120, 5);
    }
    calc ==> {
      false;
    <==>
      false && true;
      { assert 100 + 25 == 120 + 5; }
      true;
    }
}
